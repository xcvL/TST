{
    "manifest_version" : 3,
    "name" : "FastTrans",
    "version" : "1.0",
    "description" : "With FastTrans, just drag to highlight text and see the translation instantly in the console.",
    "icons" : {
        "16" : "icons/icon16.png",
        "48" : "icons/icon48.png",
        "128" : "icons/icon128.png"
    },
    "permissions" : ["activeTab"],
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["index.js"]
        }
    ]
}